      "module": "commonjs",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "pretty": true,
      "sourceMap": true,
      "target": "es2018",
      "resolveJsonModule": true,
      "outDir": "./dist",
      "allowJs": true,
  },
  "include": [
      "**/*.ts",
  ],
  "exclude": [
      "node_modules",
  ]
}
